{
    "name": "Switch/Open Google Calendar Tab",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Select Google Calendar tab on Google Chrome.",
    "permissions": [
        "tabs"
    ],
    "icons": {
        "128": "/image/google_calendar.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "commands": {
        "toggle_function": {
            "suggested_key": {
                "default": "Ctrl+Shift+C",
                "mac": "MacCtrl+Shift+C"
            },
            "description": "Toggle function by commands",
            "global": false
        }
    }
}